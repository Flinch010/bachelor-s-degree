    "TestParameters"   : ["Test", "N", "MaxAError"],    	
    "ResultParameters" : ["Tmax", "Tmin", "Tavg", "Tmed","Tsum", "iTmax", "iTmin", "iTavg", "iTmed", "iTsum", "Check - transform", "Check - inverse transform", "Emax", "Emin", "Eavg"],    	
  
